<?xml version="1.0" encoding="utf-8" ?>
<Controls:CustomControl xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:tabView="clr-namespace:Syncfusion.Maui.TabView;assembly=Syncfusion.Maui.TabView"
             xmlns:Controls="clr-namespace:Cardrly.Controls"
             xmlns:zxing="clr-namespace:ZXing.Net.Maui.Controls;assembly=ZXing.Net.MAUI.Controls"
             xmlns:progressBar="clr-namespace:Syncfusion.Maui.ProgressBar;assembly=Syncfusion.Maui.ProgressBar"
             xmlns:chart="clr-namespace:Syncfusion.Maui.Charts;assembly=Syncfusion.Maui.Charts"
             xmlns:vm="clr-namespace:Cardrly.ViewModels"
             xmlns:Lvm="clr-namespace:Cardrly.ViewModels.Leads"
             xmlns:res="clr-namespace:Cardrly.Resources.Lan"
             xmlns:xct="http://schemas.microsoft.com/dotnet/2022/maui/toolkit"
             x:Class="Cardrly.Pages.HomePage"
             Title="HomePage"
             IsEnabled="{Binding IsEnable}">

    <StackLayout>
        <tabView:SfTabView x:Name="tabHome" IndicatorPlacement="Bottom" TabBarPlacement="Bottom" IndicatorBackground="#FF7F3E" FlowDirection="LeftToRight"
                           VerticalOptions="FillAndExpand" Margin="0,0,0,10" ContentTransitionDuration="300" SelectionChanged="SfTabView_SelectionChanged">
            <tabView:SfTabView.Items>
                <!--Home-->
                <tabView:SfTabItem>
                    <tabView:SfTabItem.ImageSource>
                        <FontImageSource Glyph="" FontFamily="FontIconSolid" Color="Gray"></FontImageSource>
                    </tabView:SfTabItem.ImageSource>
                    <tabView:SfTabItem.Content>
                        <ContentView x:Name="HomeView" IsEnabled="{Binding IsEnable}" HorizontalOptions="FillAndExpand">
                            <StackLayout Spacing="10">
                                <!--Header-->
                                <StackLayout Orientation="Horizontal" Margin="20,20,20,0">
                                    <Label Text="{x:Static res:AppResources.hdrHome}" FontAutoScalingEnabled="False" FontSize="18" FontAttributes="Bold" HorizontalOptions="CenterAndExpand" TextColor="Black"></Label>
                                    <Image x:Name="LoadIcon">
                                        <Image.Source>
                                            <FontImageSource Glyph="" FontFamily="FontIconSolid" Size="22" Color="#5ac465"></FontImageSource>
                                        </Image.Source>
                                        <Image.Triggers>
                                            <DataTrigger TargetType="Image" Binding="{Binding IsCheckOrGo}" Value="2">
                                                <Setter Property="Source">
                                                    <Setter.Value>
                                                        <FontImageSource Glyph="" FontFamily="FontIconBrand" Size="32" Color="#FF7F3E"></FontImageSource>
                                                    </Setter.Value>
                                                </Setter>
                                            </DataTrigger>
                                        </Image.Triggers>
                                        <Image.GestureRecognizers>
                                            <TapGestureRecognizer Command="{Binding GetClickCommand}"></TapGestureRecognizer>
                                        </Image.GestureRecognizers>
                                    </Image>
                                </StackLayout>

                                <!--Data Menu-->
                                <StackLayout Orientation="Horizontal" Spacing="10" Margin="20,0,20,0" HorizontalOptions="FillAndExpand">
                                    <!--From-->
                                    <VerticalStackLayout Spacing="5">
                                        <Label Text="{x:Static res:AppResources.lblFrom}" TextColor="Gray" FontSize="13"></Label>
                                        <Border StrokeShape="RoundRectangle 10" StrokeThickness="1" BackgroundColor="White" Stroke="Gray" HeightRequest="40">
                                            <DatePicker TextColor="Black" FontSize="14" FontAutoScalingEnabled="False" Margin="0,0,20,0" Date="{Binding FromDate}" DateSelected="DatePicker_Home"></DatePicker>
                                        </Border>

                                    </VerticalStackLayout>
                                    <!--To-->
                                    <VerticalStackLayout Spacing="5">
                                        <Label Text="{x:Static res:AppResources.lblTo}" TextColor="Gray" FontSize="13"></Label>
                                        <Border StrokeShape="RoundRectangle 10" StrokeThickness="1" BackgroundColor="White" Stroke="Gray" HeightRequest="40">
                                            <DatePicker TextColor="Black" FontSize="14" FontAutoScalingEnabled="False" Margin="0,0,20,0" Date="{Binding ToDate}" DateSelected="DatePicker_Home"></DatePicker>
                                        </Border>
                                    </VerticalStackLayout>
                                    <!--Select Card-->
                                    <VerticalStackLayout Spacing="5" HorizontalOptions="FillAndExpand">
                                        <Label Text="{x:Static res:AppResources.lblCard}" TextColor="Gray" FontSize="13"></Label>
                                        <Border StrokeShape="RoundRectangle 10" StrokeThickness="1" BackgroundColor="White" Stroke="Gray" HeightRequest="40">
                                            <Picker x:Name="CardPicker" TextColor="Black" FontSize="14" FontAutoScalingEnabled="False" Margin="0,0,20,0" ItemDisplayBinding="{Binding CardName}" ItemsSource="{Binding CardLst}" SelectedIndexChanged="CardPicker_Home">
                                                <Picker.Triggers>
                                                    <DataTrigger TargetType="Picker" Binding="{Binding IsFoundCards}" Value="true">
                                                        <Setter Property="SelectedItem" Value='{Binding SelectedCard}'></Setter>
                                                    </DataTrigger>
                                                </Picker.Triggers>
                                            </Picker>
                                        </Border>
                                    </VerticalStackLayout>
                                </StackLayout>

                                <!--Data-->
                                <RefreshView x:Name="HomeRef" VerticalOptions="FillAndExpand" Refreshing="HomeRef_Refreshing">
                                    <ScrollView VerticalOptions="FillAndExpand">
                                        <StackLayout Spacing="20">

                                            <!--progressBars-->
                                            <Border StrokeShape="RoundRectangle 20" Padding="15" Stroke="Black" BackgroundColor="White" Margin="20,0,20,0">
                                                <VerticalStackLayout Spacing="20">
                                                    <!--progressBar For Card-->
                                                    <VerticalStackLayout Spacing="10">
                                                        <StackLayout Orientation="Horizontal" Spacing="3">
                                                            <Label Text="{x:Static res:AppResources.lblCard}" FontAutoScalingEnabled="False" FontSize="13" TextColor="Gray"></Label>
                                                            <Label Text="{Binding AccResponse.CurrentCountCards}" FontAutoScalingEnabled="False" FontSize="13" TextColor="Gray" HorizontalOptions="EndAndExpand"></Label>
                                                            <Label Text="{x:Static res:AppResources.lblFrom}" FontAutoScalingEnabled="False" FontSize="13" TextColor="Gray"></Label>
                                                            <Label Text="{Binding AccResponse.CountCards}" FontAutoScalingEnabled="False" FontSize="13" TextColor="Gray"></Label>
                                                        </StackLayout>
                                                        <progressBar:SfLinearProgressBar Progress="{Binding AccResponse.CardProgress}" AnimationEasing="{x:Static Easing.SpringOut}" SecondaryAnimationDuration="100"/>
                                                    </VerticalStackLayout>
                                                    <!--progressBar For users-->
                                                    <VerticalStackLayout Spacing="10">
                                                        <StackLayout Orientation="Horizontal" Spacing="3">
                                                            <Label Text="{x:Static res:AppResources.lblUsers}" FontAutoScalingEnabled="False" FontSize="13" TextColor="Gray"></Label>
                                                            <Label Text="{Binding AccResponse.CurrentCountUsers}" FontAutoScalingEnabled="False" FontSize="13" TextColor="Gray" HorizontalOptions="EndAndExpand"></Label>
                                                            <Label Text="{x:Static res:AppResources.lblFrom}" FontAutoScalingEnabled="False" FontSize="13" TextColor="Gray"></Label>
                                                            <Label Text="{Binding AccResponse.CountUsers}" FontAutoScalingEnabled="False" FontSize="13" TextColor="Gray"></Label>
                                                        </StackLayout>
                                                        <progressBar:SfLinearProgressBar Progress="{Binding AccResponse.UsersProgress}" AnimationEasing="{x:Static Easing.SpringOut}" SecondaryAnimationDuration="100"/>
                                                    </VerticalStackLayout>
                                                    <!--progressBar for Expire-->
                                                    <VerticalStackLayout Spacing="10" >
                                                        <StackLayout Orientation="Horizontal" Spacing="3">
                                                            <Label Text="{x:Static res:AppResources.lblExpire}" FontAutoScalingEnabled="False" FontSize="13" TextColor="Gray"></Label>
                                                            <Label Text="{Binding AccResponse.RemmingDays}" FontAutoScalingEnabled="False" FontSize="13" TextColor="Gray" HorizontalOptions="EndAndExpand"></Label>
                                                            <Label Text="{x:Static res:AppResources.lblFrom}" FontAutoScalingEnabled="False" FontSize="13" TextColor="Gray"></Label>
                                                            <Label Text="{Binding AccResponse.DayOperationAcc}" FontAutoScalingEnabled="False" FontSize="13" TextColor="Gray"></Label>
                                                        </StackLayout>

                                                        <progressBar:SfLinearProgressBar  Progress="{Binding AccResponse.ExpireProgress}" AnimationEasing="{x:Static Easing.SpringOut}" SecondaryAnimationDuration="100"/>
                                                    </VerticalStackLayout>
                                                </VerticalStackLayout>
                                            </Border>
                                            
                                            
                                            <!--Statistics Data-->
                                            <StackLayout Orientation="Horizontal" HorizontalOptions="FillAndExpand" Spacing="20" Margin="20,0,20,0">
                                                <!--Leads Register-->
                                                <Border StrokeShape="RoundRectangle 10" Padding="10" HorizontalOptions="FillAndExpand" Stroke="Black" BackgroundColor="White">
                                                    <ScrollView Orientation="Horizontal">
                                                        <StackLayout Orientation="Horizontal" ToolTipProperties.Text="{Binding BoardResponse.CountLeads}">
                                                            <!--Image-->
                                                            <Image Aspect="AspectFill" Margin="0,0,5,0">
                                                                <Image.Source>
                                                                    <FontImageSource Glyph="" FontFamily="FontIconSolid" Color="OrangeRed" Size="20"></FontImageSource>
                                                                </Image.Source>
                                                            </Image>
                                                            <!--Titel-->
                                                            <Label Text="{x:Static res:AppResources.lblLeadsRegistered}" TextColor="Black" FontSize="14" VerticalTextAlignment="Center"></Label>
                                                            <Label Text=" : " TextColor="Black" FontSize="14" VerticalTextAlignment="Center"></Label>
                                                            <Label Text="{Binding BoardResponse.CountLeads}" TextColor="Black" FontSize="14" FontAttributes="Bold" VerticalTextAlignment="Center"></Label>
                                                        </StackLayout>
                                                    </ScrollView>
                                                </Border>
                                                <!--Card Views-->
                                                <Border StrokeShape="RoundRectangle 10" Padding="10" HorizontalOptions="FillAndExpand" Stroke="Black" BackgroundColor="White">
                                                    <ScrollView Orientation="Horizontal">
                                                        <StackLayout Orientation="Horizontal" ToolTipProperties.Text="{Binding BoardResponse.CountViewCard}">
                                                            <!--Image-->
                                                            <Image Aspect="AspectFill" Margin="0,0,5,0">
                                                                <Image.Source>
                                                                    <FontImageSource Glyph="" FontFamily="FontIconSolid" Color="OrangeRed" Size="20"></FontImageSource>
                                                                </Image.Source>
                                                            </Image>
                                                            <!--Titel-->
                                                            <Label Text="{x:Static res:AppResources.lblCardViews}" TextColor="Black" FontSize="14" VerticalTextAlignment="Center"></Label>
                                                            <Label Text=" : " TextColor="Black" FontSize="14" VerticalTextAlignment="Center"></Label>
                                                            <Label Text="{Binding BoardResponse.CountViewCard}" TextColor="Black" FontSize="14" FontAttributes="Bold" VerticalTextAlignment="Center"></Label>
                                                        </StackLayout>
                                                    </ScrollView>
                                                </Border>
                                            </StackLayout>
                                            
                                            
                                            <StackLayout Orientation="Horizontal" HorizontalOptions="FillAndExpand" Spacing="20" Margin="20,0,20,0">
                                                <!--Link Taps-->
                                                <Border StrokeShape="RoundRectangle 10" Padding="10" HorizontalOptions="FillAndExpand" Stroke="Black" BackgroundColor="White">
                                                    <ScrollView Orientation="Horizontal">
                                                        <StackLayout Orientation="Horizontal" ToolTipProperties.Text="{Binding BoardResponse.CountLinkTap}">
                                                            <!--Image-->
                                                            <Image Aspect="AspectFill" Margin="0,0,5,0">
                                                                <Image.Source>
                                                                    <FontImageSource Glyph="" FontFamily="FontIconSolid" Color="OrangeRed" Size="20"></FontImageSource>
                                                                </Image.Source>
                                                            </Image>
                                                            <!--Titel-->
                                                            <Label Text="{x:Static res:AppResources.lblLinkClicks}" TextColor="Black" FontSize="14" VerticalTextAlignment="Center"></Label>
                                                            <Label Text=" : " TextColor="Black" FontSize="14" VerticalTextAlignment="Center"></Label>
                                                            <Label Text="{Binding BoardResponse.CountLinkTap}" TextColor="Black" FontSize="14" FontAttributes="Bold" VerticalTextAlignment="Center"></Label>
                                                        </StackLayout>
                                                    </ScrollView>
                                                </Border>
                                                <!--Save Contacts-->
                                                <Border StrokeShape="RoundRectangle 10" Padding="10" HorizontalOptions="FillAndExpand" Stroke="Black" BackgroundColor="White">
                                                    <ScrollView Orientation="Horizontal">
                                                        <StackLayout Orientation="Horizontal" ToolTipProperties.Text="{Binding BoardResponse.CountDownloadContacts}">
                                                            <!--Image-->
                                                            <Image Aspect="AspectFill" Margin="0,0,5,0">
                                                                <Image.Source>
                                                                    <FontImageSource Glyph="" FontFamily="FontIconSolid" Color="OrangeRed" Size="20"></FontImageSource>
                                                                </Image.Source>
                                                            </Image>
                                                            <!--Titel-->
                                                            <Label Text="{x:Static res:AppResources.lblSaveContacts}" TextColor="Black" FontSize="14" VerticalTextAlignment="Center"></Label>
                                                            <Label Text=" : " TextColor="Black" FontSize="14" VerticalTextAlignment="Center"></Label>
                                                            <Label Text="{Binding BoardResponse.CountDownloadContacts}" TextColor="Black" FontSize="14" FontAttributes="Bold" VerticalTextAlignment="Center"></Label>
                                                        </StackLayout>
                                                    </ScrollView>
                                                </Border>
                                            </StackLayout>
                                            
                                            
                                            
                                            <!--Total Clicks Chart-->
                                            <Border StrokeShape="RoundRectangle 20" Padding="15" Stroke="Black" BackgroundColor="White" Margin="20,0,20,0">
                                                <chart:SfCartesianChart HeightRequest="250">
                                                    <chart:SfCartesianChart.XAxes>
                                                        <chart:CategoryAxis>

                                                        </chart:CategoryAxis>
                                                    </chart:SfCartesianChart.XAxes>
                                                    <chart:SfCartesianChart.YAxes>
                                                        <chart:NumericalAxis>

                                                        </chart:NumericalAxis>
                                                    </chart:SfCartesianChart.YAxes>
                                                    <chart:SfCartesianChart.Title>
                                                        <Label Text="{x:Static res:AppResources.lblTotalClicks}" FontSize="14" TextColor="Black" FontAttributes="Bold" HorizontalTextAlignment="Center"></Label>
                                                    </chart:SfCartesianChart.Title>
                                                    <chart:FastLineSeries ItemsSource="{Binding BoardResponse.clickCardSummaries}" EnableTooltip="True" EnableAnimation="True" XBindingPath="DateClick" YBindingPath="CountClickCard"/>
                                                </chart:SfCartesianChart>
                                            </Border>

                                            <!--First Tab-->
                                            <tabView:SfTabView  IndicatorPlacement="Fill" TabBarPlacement="Top" IndicatorBackground="#fae6dc" TabBarHeight="50" VerticalOptions="FillAndExpand" FlowDirection="LeftToRight" Margin="0,0,0,0">
                                                <tabView:SfTabView.Items>
                                                    <!--Tab 1-->
                                                    <tabView:SfTabItem BackgroundColor="Transparent" Header="{x:Static res:AppResources.lblDevices}" FontSize="14" FontAttributes="Bold">
                                                        <tabView:SfTabItem.Content>
                                                            <ContentView VerticalOptions="FillAndExpand" Margin="20,10,20,0">
                                                                <ScrollView VerticalOptions="FillAndExpand">
                                                                    <StackLayout Spacing="15" Margin="0,5,0,0">

                                                                        <!--DoughnutSeries-->
                                                                        <Border StrokeShape="RoundRectangle 20" Padding="15" Stroke="Black" BackgroundColor="White">
                                                                            <chart:SfCircularChart>
                                                                                <chart:DoughnutSeries ItemsSource="{Binding BoardResponse.clickCardLinkSummariesOS}" XBindingPath="name" YBindingPath="value"  ExplodeOnTouch="True" EnableAnimation="True" EnableTooltip="True"/>
                                                                                <chart:SfCircularChart.Legend>
                                                                                    <chart:ChartLegend >
                                                                                        <chart:ChartLegend.LabelStyle>
                                                                                            <chart:ChartLegendLabelStyle TextColor="Black" Margin="5" FontSize="14" FontAttributes="Bold"  FontFamily="PlaywriteAR-Regular"/>
                                                                                        </chart:ChartLegend.LabelStyle>
                                                                                    </chart:ChartLegend>
                                                                                </chart:SfCircularChart.Legend>
                                                                                <chart:SfCircularChart.Title>
                                                                                    <Label Text="{x:Static res:AppResources.lblTCDS}" FontSize="14" TextColor="Black" FontAttributes="Bold" HorizontalTextAlignment="Center"></Label>
                                                                                </chart:SfCircularChart.Title>
                                                                            </chart:SfCircularChart>
                                                                        </Border>
                                                                    </StackLayout>
                                                                </ScrollView>
                                                            </ContentView>
                                                        </tabView:SfTabItem.Content>
                                                    </tabView:SfTabItem>
                                                    <!--Tab 2-->
                                                    <tabView:SfTabItem BackgroundColor="Transparent" Header="{x:Static res:AppResources.lblEngagements}" FontSize="14" FontAttributes="Bold">
                                                        <tabView:SfTabItem.Content>
                                                            <ContentView Margin="20,10,20,0">
                                                                <StackLayout>
                                                                    <ScrollView VerticalOptions="FillAndExpand">
                                                                        <StackLayout Spacing="15" Margin="0,5,0,0">

                                                                            <!--User Link Engagements-->
                                                                            <!--Table Headers-->
                                                                            <Border StrokeShape="RoundRectangle 20" Padding="15" Stroke="Black" BackgroundColor="White" VerticalOptions="FillAndExpand" Margin="0,0,0,20"  HorizontalOptions="FillAndExpand">
                                                                                <StackLayout Spacing="15" VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand">
                                                                                    <Label Text="{x:Static res:AppResources.lblUserLinkEngagements}" FontSize="14" TextColor="Black" FontAttributes="Bold" HorizontalTextAlignment="Center"></Label>
                                                                                    <Grid ColumnDefinitions="*,*,*,0.5,*" HorizontalOptions="FillAndExpand">
                                                                                        <Label Text="{x:Static res:AppResources.hdrImage}" Grid.Column="0" FontAutoScalingEnabled="False" FontSize="14" FontAttributes="Bold" HorizontalOptions="CenterAndExpand" TextColor="Black"></Label>
                                                                                        <Label Text="{x:Static res:AppResources.hdrLink}" FontAutoScalingEnabled="False" FontSize="14" FontAttributes="Bold" HorizontalOptions="CenterAndExpand" Grid.Column="1" TextColor="Black"></Label>
                                                                                        <!--<Label Text="User" FontAutoScalingEnabled="False" FontSize="14" FontAttributes="Bold" HorizontalOptions="CenterAndExpand"  Grid.Column="2" TextColor="Black"></Label>-->
                                                                                        <Label Text="{x:Static res:AppResources.hdrCardName}" FontAutoScalingEnabled="False" FontSize="14" FontAttributes="Bold" HorizontalOptions="CenterAndExpand" Grid.Column="2" Grid.ColumnSpan="2" TextColor="Black"></Label>
                                                                                        <Label Text="{x:Static res:AppResources.hdrTaps}" FontAutoScalingEnabled="False" FontSize="14" FontAttributes="Bold" HorizontalOptions="CenterAndExpand" Grid.Column="4" TextColor="Black"></Label>

                                                                                    </Grid>
                                                                                    <BoxView Background="Gray" HeightRequest="1"></BoxView>
                                                                                    <CollectionView ItemsSource="{Binding BoardResponse.clickCardLinkSummaries}" VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand">
                                                                                        <CollectionView.ItemsLayout>
                                                                                            <GridItemsLayout Orientation="Vertical" VerticalItemSpacing="10"></GridItemsLayout>
                                                                                        </CollectionView.ItemsLayout>
                                                                                        <CollectionView.ItemTemplate>
                                                                                            <DataTemplate>
                                                                                                <Grid ColumnDefinitions="*,*,*,0.5,*" HorizontalOptions="FillAndExpand">
                                                                                                    <Image Source="{Binding ImgCardLinkVM}" WidthRequest="30"></Image>
                                                                                                    <Label Text="{Binding LinkName}" FontAutoScalingEnabled="False" FontSize="14" FontAttributes="Bold" HorizontalOptions="FillAndExpand" Grid.Column="1" TextColor="Black" VerticalOptions="CenterAndExpand"></Label>
                                                                                                    <!--<Label Text="{Binding PoplUser}" FontAutoScalingEnabled="False" FontSize="14" FontAttributes="Bold" HorizontalOptions="FillAndExpand" Grid.Column="2" TextColor="Black" VerticalOptions="CenterAndExpand"></Label>-->
                                                                                                    <Label Text="{Binding CardName}" FontAutoScalingEnabled="False" FontSize="14" FontAttributes="Bold" HorizontalOptions="EndAndExpand" Grid.Column="2" Grid.ColumnSpan="2" TextColor="Black" VerticalOptions="CenterAndExpand"></Label>
                                                                                                    <Label Text="{Binding CountClickCardLink}" FontAutoScalingEnabled="False" FontSize="14" FontAttributes="Bold" HorizontalOptions="CenterAndExpand" Grid.Column="4" TextColor="Black" VerticalOptions="CenterAndExpand"></Label>
                                                                                                </Grid>
                                                                                            </DataTemplate>
                                                                                        </CollectionView.ItemTemplate>
                                                                                    </CollectionView>
                                                                                </StackLayout>
                                                                            </Border>
                                                                        </StackLayout>
                                                                    </ScrollView>
                                                                </StackLayout>
                                                            </ContentView>
                                                        </tabView:SfTabItem.Content>
                                                    </tabView:SfTabItem>


                                                    <!--Tab 3-->
                                                    <tabView:SfTabItem BackgroundColor="Transparent" Header="{x:Static res:AppResources.lblCardView}" FontSize="14" FontAttributes="Bold">
                                                        <tabView:SfTabItem.Content>
                                                            <ContentView VerticalOptions="FillAndExpand" Margin="20,10,20,0">
                                                                <ScrollView VerticalOptions="FillAndExpand">
                                                                    <StackLayout Spacing="15" Margin="0,5,0,0">

                                                                        <!--DoughnutSeries-->
                                                                        <Border StrokeShape="RoundRectangle 20" Padding="15" Stroke="Black" BackgroundColor="White">
                                                                            <chart:SfCircularChart>
                                                                                <chart:DoughnutSeries ItemsSource="{Binding BoardResponse.clickCardSummariesCountry}" XBindingPath="name" YBindingPath="value"  ExplodeOnTouch="True" EnableAnimation="True" EnableTooltip="True"/>
                                                                                <chart:SfCircularChart.Legend>
                                                                                    <chart:ChartLegend >
                                                                                        <chart:ChartLegend.LabelStyle>
                                                                                            <chart:ChartLegendLabelStyle TextColor="Black" Margin="5" FontSize="14" FontAttributes="Bold"  FontFamily="PlaywriteAR-Regular"/>
                                                                                        </chart:ChartLegend.LabelStyle>
                                                                                    </chart:ChartLegend>
                                                                                </chart:SfCircularChart.Legend>
                                                                                <chart:SfCircularChart.Title>
                                                                                    <Label Text="{x:Static res:AppResources.lbl_CardView_Distribution_by_Country}" FontSize="14" TextColor="Black" FontAttributes="Bold" HorizontalTextAlignment="Center"></Label>
                                                                                </chart:SfCircularChart.Title>
                                                                            </chart:SfCircularChart>
                                                                        </Border>
                                                                    </StackLayout>
                                                                </ScrollView>
                                                            </ContentView>
                                                        </tabView:SfTabItem.Content>
                                                    </tabView:SfTabItem>
                                                    <!--Tab 4-->
                                                    <tabView:SfTabItem BackgroundColor="Transparent" Header="{x:Static res:AppResources.lblLinkClicks}" FontSize="14" FontAttributes="Bold">
                                                        <tabView:SfTabItem.Content>
                                                            <ContentView VerticalOptions="FillAndExpand" Margin="20,10,20,0">
                                                                <ScrollView VerticalOptions="FillAndExpand">
                                                                    <StackLayout Spacing="15" Margin="0,5,0,0">

                                                                        <!--DoughnutSeries-->
                                                                        <Border StrokeShape="RoundRectangle 20" Padding="15" Stroke="Black" BackgroundColor="White">
                                                                            <chart:SfCircularChart>
                                                                                <chart:DoughnutSeries ItemsSource="{Binding BoardResponse.clickCardLinkSummariesCountry}" XBindingPath="name" YBindingPath="value"  ExplodeOnTouch="True" EnableAnimation="True" EnableTooltip="True"/>
                                                                                <chart:SfCircularChart.Legend>
                                                                                    <chart:ChartLegend >
                                                                                        <chart:ChartLegend.LabelStyle>
                                                                                            <chart:ChartLegendLabelStyle TextColor="Black" Margin="5" FontSize="14" FontAttributes="Bold"  FontFamily="PlaywriteAR-Regular"/>
                                                                                        </chart:ChartLegend.LabelStyle>
                                                                                    </chart:ChartLegend>
                                                                                </chart:SfCircularChart.Legend>
                                                                                <chart:SfCircularChart.Title>
                                                                                    <Label Text="{x:Static res:AppResources.lbl_Link_Clicks_by_Country}" FontSize="14" TextColor="Black" FontAttributes="Bold" HorizontalTextAlignment="Center"></Label>
                                                                                </chart:SfCircularChart.Title>
                                                                            </chart:SfCircularChart>
                                                                        </Border>
                                                                    </StackLayout>
                                                                </ScrollView>
                                                            </ContentView>
                                                        </tabView:SfTabItem.Content>
                                                    </tabView:SfTabItem>
                                                </tabView:SfTabView.Items>
                                            </tabView:SfTabView>

                                            <!--Second Tab--><!--
                                            <tabView:SfTabView  IndicatorPlacement="Fill" TabBarPlacement="Top" IndicatorBackground="#fae6dc" TabBarHeight="40" VerticalOptions="FillAndExpand" FlowDirection="LeftToRight" Margin="0,0,0,0">
                                                <tabView:SfTabView.Items>
                                                    --><!--Tab 1--><!--
                                                    <tabView:SfTabItem BackgroundColor="Transparent" Header="{x:Static res:AppResources.lblCardView}" FontSize="16" FontAttributes="Bold">
                                                        <tabView:SfTabItem.Content>
                                                            <ContentView VerticalOptions="FillAndExpand" Margin="20,10,20,0">
                                                                <ScrollView VerticalOptions="FillAndExpand">
                                                                    <StackLayout Spacing="15" Margin="0,5,0,0">

                                                                        --><!--DoughnutSeries--><!--
                                                                        <Border StrokeShape="RoundRectangle 20" Padding="15" Stroke="Black" BackgroundColor="White">
                                                                            <chart:SfCircularChart>
                                                                                <chart:DoughnutSeries ItemsSource="{Binding BoardResponse.clickCardSummariesCountry}" XBindingPath="name" YBindingPath="value"  ExplodeOnTouch="True" EnableAnimation="True" EnableTooltip="True"/>
                                                                                <chart:SfCircularChart.Legend>
                                                                                    <chart:ChartLegend >
                                                                                        <chart:ChartLegend.LabelStyle>
                                                                                            <chart:ChartLegendLabelStyle TextColor="Black" Margin="5" FontSize="14" FontAttributes="Bold"  FontFamily="PlaywriteAR-Regular"/>
                                                                                        </chart:ChartLegend.LabelStyle>
                                                                                    </chart:ChartLegend>
                                                                                </chart:SfCircularChart.Legend>
                                                                                <chart:SfCircularChart.Title>
                                                                                    <Label Text="{x:Static res:AppResources.lbl_CardView_Distribution_by_Country}" FontSize="14" TextColor="Black" FontAttributes="Bold" HorizontalTextAlignment="Center"></Label>
                                                                                </chart:SfCircularChart.Title>
                                                                            </chart:SfCircularChart>
                                                                        </Border>
                                                                    </StackLayout>
                                                                </ScrollView>
                                                            </ContentView>
                                                        </tabView:SfTabItem.Content>
                                                    </tabView:SfTabItem>
                                                    --><!--Tab 2--><!--
                                                    <tabView:SfTabItem BackgroundColor="Transparent" Header="{x:Static res:AppResources.lblLinkClicks}" FontSize="16" FontAttributes="Bold">
                                                        <tabView:SfTabItem.Content>
                                                            <ContentView VerticalOptions="FillAndExpand" Margin="20,10,20,0">
                                                                <ScrollView VerticalOptions="FillAndExpand">
                                                                    <StackLayout Spacing="15" Margin="0,5,0,0">

                                                                        --><!--DoughnutSeries--><!--
                                                                        <Border StrokeShape="RoundRectangle 20" Padding="15" Stroke="Black" BackgroundColor="White">
                                                                            <chart:SfCircularChart>
